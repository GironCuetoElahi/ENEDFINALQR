<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/lib/com.example.elahi.aplicacionened"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="59dp"
        android:layout_height="59dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="13dp"
        app:srcCompat="@drawable/futbol" />

    <TextView
        android:id="@+id/Equipo1"
        android:layout_width="135dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="171dp"
        android:layout_marginTop="8dp"
        android:text="TEC OAXACA"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/sede"
        android:layout_width="131dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/Equipo1"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-70dp"
        android:layout_marginTop="54dp"
        android:text="IT OAXACA"
        android:textStyle="italic" />

    <TextView
        android:id="@+id/horario"
        android:layout_width="116dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/sede"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="11dp"
        android:text="TextView" />

</RelativeLayout>